 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=outNotA);		 // Not(a) -> outNotA
    Not(in=b, out=outNotB);		 // Not(b) -> outNotB 
    Nand(a=outNotA, b=outNotB, out=out); // Nand(outNotA, outNotB) -> out
}
