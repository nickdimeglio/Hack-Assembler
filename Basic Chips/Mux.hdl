/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=outNotSel);
    And(a=sel, b=b, out=And1);
    And(a=a, b=outNotSel, out=And2);
    Or(a=And1, b=And2, out=out);
}



