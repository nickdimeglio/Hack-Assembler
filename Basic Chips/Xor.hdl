/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=outNotA);
    Not(in=b, out=outNotB);
    And(a=a, b=outNotB, out=outNot1);
    And(b=b, a=outNotA, out=outNot2);
    Or(a=outNot1, b=outNot2, out=out);
}